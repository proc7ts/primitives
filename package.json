{
  "name": "@proc7ts/primitives",
  "version": "1.8.0",
  "description": "Type and function primitives",
  "keywords": [
    "primitives",
    "function",
    "typescript"
  ],
  "homepage": "https://github.com/proc7ts/primitives",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:proc7ts/primitives.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/proc7ts/primitives/issues"
  },
  "type": "module",
  "main": "./dist/primitives.cjs",
  "module": "./dist/primitives.js",
  "types": "./index.d.ts",
  "exports": {
    "import": "./dist/primitives.js",
    "require": "./dist/primitives.cjs"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@run-z/eslint-config": "^1.2.1",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "codecov": "^3.8.1",
    "eslint": "^7.19.0",
    "eslint-plugin-jest": "^24.1.3",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "rollup": "^2.38.5",
    "rollup-plugin-flat-dts": "^1.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.29.0",
    "run-z": "=1.6.1-bootstrap",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.0",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.21",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "bootstrap": "rollup --config ./rollup.config.js",
    "build": "run-z +z bootstrap",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf index.d.ts dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "lint": "run-z +z --then eslint .",
    "test": "run-z +z --then jest",
    "z": "run-z +bootstrap,+doc,+lint,+test"
  }
}
